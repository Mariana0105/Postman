	"item": [
		{
			"name": "Add Place",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function (){\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"pm.test(\"Set placeID key in Env\", function (){\r",
							"   var responseData = pm.response.json();\r",
							"   pm.environment.set(\"placeID\", responseData.place_id);\r",
							"});\r",
							"    \r",
							"  \r",
							"\r",
							"\r",
							" \r",
							"\r",
							"\r",
							"\r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n  \"location\": {\r\n    \"lat\": -38.383494,\r\n    \"lng\": 33.427362\r\n  },\r\n  \"accuracy\": 50,\r\n  \"name\": \"Frontline house\",\r\n  \"phone_number\": \"(+91) 983 893 3937\",\r\n  \"address\": \"29, side layout, cohen 09\",\r\n  \"types\": [\r\n    \"shoe park\",\r\n    \"shop\"\r\n  ],\r\n  \"website\": \"http://google.com\",\r\n  \"language\": \"French-IN\"\r\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{url}}/maps/api/place/add/json?key={{key}}",
					"host": [
						"{{url}}"
					],
					"path": [
						"maps",
						"api",
						"place",
						"add",
						"json"
					],
					"query": [
						{
							"key": "key",
							"value": "{{key}}"
						}
					]
				}
			},
			"response": []
		},
		{
			"name": "Get Place",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Verification status code\", function (){\r",
							"    if (pm.environment.get(\"placeID\")){\r",
							"      pm.response.to.have.status(200);\r",
							"      pm.test(\"Verify if the correct address returns\", function (){\r",
							"         var jsonData = pm.response.json();\r",
							"         pm.expect(jsonData.address).to.eql(\"29, side layout, cohen 09\");\r",
							"});\r",
							"    } else {\r",
							"      pm.response.to.have.status(404);\r",
							"      postman.setNextRequest(null);\r",
							"    }\r",
							"});\r",
							"\r",
							"\r",
							"\r",
							"\r",
							"\r",
							"\r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
